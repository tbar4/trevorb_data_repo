# Example .env file for the API CLI application

# API Configuration
API_BASE_URL=https://api.spaceflightnewsapi.net/v4
API_KEY=your_api_key_here

# Database Configuration
DATABASE_URL=sqlite://data.db
POSTGRES_URL=postgresql://user:password@localhost/database

# S3 Configuration
S3_BUCKET=my-bucket
S3_REGION=us-east-1

# Output Configuration
DEFAULT_OUTPUT_TYPE=local
DEFAULT_OUTPUT_PATH=./data